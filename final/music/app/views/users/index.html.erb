<style>
table, th, td {
  border: 1px solid black;
  border-collapse: collapse;
  background-color: whitesmoke;
  padding: 5px;
  text-align: center;
}
th {
  background-color: navy;
}
td {
  background-color: whitesmoke;
}
</style>

<table align="center" width="25%"><tr><th><h3><font color="white">Listing Users</font></h3></th></tr>
<tr><td>
<ul>
  <% @users.each do |user| %>
    <li><%= user.name %> | Registered On: <%= user.created_at %></li>
  <% end %>
</ul>
<%= link_to 'Back to Songs', songs_path %>
</td></tr></table>
